{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2021",
    "declaration": false,
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "strict": true,
    "strictFunctionTypes": false,
    "experimentalDecorators": true
  }
}
